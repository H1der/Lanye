.container {
  padding: 100rpx 0;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.container text{
    color: #454545;
    line-height: 1.8;
    padding: 2vh 0;
}

.container image{
  height: 600rpx;
  width: 600rpx;
}